package com.experion.assignment1;

public class Pattern_10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int size = 4;
		for (int i = 0 ;i<size;i++) {
			System.out.print(i+1);
			for (int j=0;j<=i;j++) {
				System.out.print(i+1);
				System.out.print(" ");
			}
			System.out.println();
		}


	}

}
